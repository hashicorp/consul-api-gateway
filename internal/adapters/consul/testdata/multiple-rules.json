{
  "Name": "multiple-rules",
  "Namespace": "k8s",
  "Hostnames": [
    "example.com"
  ],
  "Rules": [
    {
      "Matches": [
        {
          "Path": {
            "Type": "HTTPPathMatchExact",
            "Value": "/prefix-1"
          }
        },
        {
          "Path": {
            "Type": "HTTPPathMatchExact",
            "Value": "/other-1"
          }
        }
      ],
      "Filters": [
        {
          "Type": "HTTPHeaderFilter",
          "Header": {
            "Set": {
              "x-set": "1"
            },
            "Add": {
              "x-add": "2"
            },
            "Remove": [
              "x-remove"
            ]
          }
        },
        {
          "Type": "HTTPHeaderFilter",
          "Header": {
            "Set": {
              "x-set-too": "1"
            },
            "Add": {
              "x-add-too": "2"
            }
          }
        }
      ],
      "Services": [
        {
          "Service": {
            "ConsulNamespace": "namespace",
            "Service": "service"
          },
          "Weight": 1,
          "Filters": [
            {
              "Type": "HTTPHeaderFilter",
              "Header": {
                "Set": {
                  "x-set": "3"
                },
                "Add": {
                  "x-add": "4"
                },
                "Remove": [
                  "x-remove-a"
                ]
              }
            }
          ]
        },
        {
          "Service": {
            "ConsulNamespace": "namespace",
            "Service": "another-service"
          },
          "Weight": 1,
          "Filters": [
            {
              "Type": "HTTPHeaderFilter",
              "Header": {
                "Add": {
                  "x-add": "4"
                },
                "Remove": [
                  "x-remove-b"
                ]
              }
            }
          ]
        }
      ]
    },
    {
      "Matches": [
        {
          "Path": {
            "Type": "HTTPPathMatchExact",
            "Value": "/prefix-2"
          }
        }
      ],
      "Filters": [
        {
          "Type": "HTTPHeaderFilter",
          "Header": {
            "Set": {
              "x-set-2": "5"
            },
            "Add": {
              "x-add-2": "6"
            },
            "Remove": [
              "x-remove-2"
            ]
          }
        },
        {
          "Type": "HTTPHeaderFilter",
          "Header": {
            "Set": {
              "x-set-2-too": "7"
            },
            "Add": {
              "x-add-2-too": "8"
            }
          }
        }
      ],
      "Services": [
        {
          "Service": {
            "ConsulNamespace": "namespace",
            "Service": "service"
          },
          "Weight": 1,
          "Filters": [
            {
              "Type": "HTTPHeaderFilter",
              "Header": {
                "Set": {
                  "x-set-2": "6"
                },
                "Add": {
                  "x-add-2": "7"
                },
                "Remove": [
                  "x-remove-a"
                ]
              }
            }
          ]
        },
        {
          "Service": {
            "ConsulNamespace": "namespace",
            "Service": "another-service"
          },
          "Weight": 1,
          "Filters": [
            {
              "Type": "HTTPHeaderFilter",
              "Header": {
                "Add": {
                  "x-add": "4"
                },
                "Remove": [
                  "x-remove-b"
                ]
              }
            }
          ]
        }
      ]
    }
  ]
}