{
  "Name": "multiple-services",
  "Hostnames": [
    "example.com"
  ],
  "Rules": [
    {
      "Matches": [
        {
          "Type": 1,
          "Path": {
            "Type": 1,
            "Value": "/prefix"
          }
        }
      ],
      "Filters": [
        {
          "Type": 0,
          "Header": {
            "Set": {
              "x-set": "1"
            },
            "Add": {
              "x-add": "2"
            },
            "Remove": [
              "x-remove"
            ]
          }
        },
        {
          "Type": 0,
          "Header": {
            "Set": {
              "x-set-too": "1"
            },
            "Add": {
              "x-add-too": "2"
            }
          }
        }
      ],
      "Services": [
        {
          "Service": {
            "ConsulNamespace": "namespace",
            "Service": "service"
          },
          "Weight": 1,
          "Filters": [
            {
              "Type": 0,
              "Header": {
                "Set": {
                  "x-set": "3"
                },
                "Add": {
                  "x-add": "4"
                },
                "Remove": [
                  "x-remove-a"
                ]
              }
            }
          ]
        },
        {
          "Service": {
            "ConsulNamespace": "namespace",
            "Service": "another-service"
          },
          "Weight": 1,
          "Filters": [
            {
              "Type": 0,
              "Header": {
                "Add": {
                  "x-add": "4"
                },
                "Remove": [
                  "x-remove-b"
                ]
              }
            }
          ]
        }
      ]
    }
  ]
}